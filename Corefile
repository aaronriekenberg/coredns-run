domain {
  file db.domain
  #log
  errors
}

1.168.192.in-addr.arpa {
  file db.192.168.1
  #log
  errors
}

. {
    forward . tls://1.1.1.1 tls://1.0.0.1 {
      tls_servername tls.cloudflare-dns.com
      health_check 5s
    }
    cache {
      success 10000 3600 300
      denial 10000 1800 150
    }
    #log
    errors
}
